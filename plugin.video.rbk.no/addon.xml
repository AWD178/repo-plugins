<?xml version="1.0" encoding="UTF-8" standalone="yes"?>
<addon id="plugin.video.rbk.no"
       name="RBK.no"
       version="1.0.0"
       provider-name="spiff">
  <requires>
    <import addon="xbmc.python" version="1.0"/>
  </requires>
  <extension point="xbmc.python.pluginsource"
             provides="video"
             library="default.py">
    <provides>video</provides>
  </extension>
  <extension point="xbmc.addon.metadata">
    <summary>See videos from RBK.no</summary>
    <description>See videos from RBK.no, the homepage of Rosenborg, the football club.</description>
    <platform>all</platform>
  </extension>
</addon>
