<?xml version="1.0" encoding="UTF-8" standalone="yes"?>
<addon id="plugin.fox.news"
       name="Fox News"
       version="1.0.1"
       provider-name="redeyed">
    <requires>
        <import addon="xbmc.python" version="1.0"/>
    </requires>
    <extension point="xbmc.python.pluginsource"
               library="default.py">
        <provides>video</provides>
    </extension>
    <extension point="xbmc.addon.metadata">
        <platform>all</platform>
        <summary>Fox News</summary>
        <description>This XBMC plugin enables viewing many of the online feeds Fox News has available</description>
    </extension>
</addon>
